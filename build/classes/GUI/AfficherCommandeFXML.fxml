      <VBox layoutX="-40.0" prefHeight="507.0" prefWidth="109.0" style="-fx-background-color: #ff4204;" />
      <Label layoutX="179.0" layoutY="25.0" text="Liste des commandes" textAlignment="CENTER" textFill="#ff4204" textOverrun="LEADING_ELLIPSIS">
