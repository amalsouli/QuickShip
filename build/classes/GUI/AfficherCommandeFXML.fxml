<AnchorPane id="AnchorPane" focusTraversable="true" prefHeight="507.0" prefWidth="735.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.AfficherCommandeFXMLController">
