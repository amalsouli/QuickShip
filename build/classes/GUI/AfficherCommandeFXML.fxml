      <TableView fx:id="tv" accessibleRole="IMAGE_VIEW" cacheShape="false" centerShape="false" focusTraversable="false" layoutX="7.0" layoutY="95.0" prefHeight="301.0" prefWidth="591.0" scaleShape="false">
